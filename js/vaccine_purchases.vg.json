{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "width": 400,
    "background": "rgb(223, 247, 255)",
    "data": {
        "url": "https://raw.githubusercontent.com/swil0067/Assignment2/main/data/air.csv"
    },
    "encoding": {
        "x": {
            "timeUnit": "yearmonthdate",
            "field": "DATE_AS_AT",
            "title": "Date"
        },
        "y": {
            "title": "Percent"
        }
    },
    "layer": [
        {
            "mark": {
                "type": "line",
                "interpolate": "monotone",
                "color": "green"
            },
            "encoding": {
                "y": {
                    "field": "AIR_AUS_16_PLUS_FIRST_DOSE_PCT",
                    "type": "quantitative"
                },
                "tooltip": [
                    {
                        "field": "DATE_AS_AT",
                        "title": "Date",
                        "type": "temporal"
                    },
                    {
                        "field": "AIR_AUS_16_PLUS_FIRST_DOSE_PCT",
                        "title": "Percent of Pop. with First Dose",
                        "type": "quantitative"
                    }
                ]
            }
        },
        {
            "mark": {
                "type": "line",
                "interpolate": "monotone",
                "color": "blue"
            },
            "encoding": {
                "y": {
                    "field": "AIR_AUS_16_PLUS_SECOND_DOSE_PCT",
                    "type": "quantitative"
                },
                "tooltip": [
                    {
                        "field": "DATE_AS_AT",
                        "title": "Date",
                        "type": "temporal"
                    },
                    {
                        "field": "AIR_AUS_16_PLUS_SECOND_DOSE_PCT",
                        "title": "Percent of Pop. with Second Dose",
                        "type": "quantitative"
                    }
                ]
            }
        },
        {
            "mark": {
                "type": "line",
                "interpolate": "monotone",
                "color": "red"
            },
            "encoding": {
                "y": {
                    "field": "AIR_AUS_16_PLUS_THIRD_DOSE_PCT",
                    "type": "quantitative"
                },
                "tooltip": [
                    {
                        "field": "DATE_AS_AT",
                        "title": "Date",
                        "type": "temporal"
                    },
                    {
                        "field": "AIR_AUS_16_PLUS_THIRD_DOSE_PCT",
                        "title": "Percent of Pop. with Third Dose",
                        "type": "quantitative"
                    }
                ]
            }
        }
    ]
}