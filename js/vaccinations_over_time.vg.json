{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "width": 450,
    "background": "rgb(223, 247, 255)",
    "data": {
        "url": "https://raw.githubusercontent.com/swil0067/Assignment2/main/data/air.csv",
        "format": {
            "type": "csv",
            "parse": {
                "DATE_AS_AT": "date:'%d/%m/%Y'"
            }
        }
    },
    "encoding": {
        "x": {
            "field": "DATE_AS_AT",
            "title": "Date",
            "timeUnit": "yearmonthdate"
        },
        "y": {
            "title": "% of Australian Population"
        }
    },
    "layer": [
        {
            "mark": {
                "type": "line",
                "interpolate": "monotone",
                "color": "purple"
            },
            "encoding": {
                "y": {
                    "field": "AIR_AUS_16_PLUS_FIRST_DOSE_PCT",
                    "type": "quantitative"
                },
                "tooltip": [
                    {
                        "field": "DATE_AS_AT",
                        "title": "Date",
                        "timeUnit": "yearmonthdate",
                        "type": "temporal",
                        "format": "%b %d %Y"
                    },
                    {
                        "field": "AIR_AUS_16_PLUS_FIRST_DOSE_PCT",
                        "title": "Percent of Population",
                        "type": "quantitative"
                    }
                ],
                "color": {
                    "datum": "First Dose"
                }
            }
        },
        {
            "mark": {
                "type": "line",
                "interpolate": "monotone",
                "color": "blue"
            },
            "encoding": {
                "y": {
                    "field": "AIR_AUS_16_PLUS_SECOND_DOSE_PCT",
                    "type": "quantitative"
                },
                "tooltip": [
                    {
                        "field": "DATE_AS_AT",
                        "title": "Date",
                        "type": "temporal",
                        "format": "%b %d %Y"
                    },
                    {
                        "field": "AIR_AUS_16_PLUS_SECOND_DOSE_PCT",
                        "title": "Percent of Population",
                        "type": "quantitative"
                    }
                ],
                "color": {
                    "datum": "Second Dose"
                }
            }
        },
        {
            "mark": {
                "type": "line",
                "interpolate": "monotone",
                "color": "red"
            },
            "encoding": {
                "y": {
                    "field": "AIR_AUS_16_PLUS_THIRD_DOSE_PCT",
                    "type": "quantitative"
                },
                "tooltip": [
                    {
                        "field": "DATE_AS_AT",
                        "title": "Date",
                        "type": "temporal",
                        "format": "%b %d %Y"
                    },
                    {
                        "field": "AIR_AUS_16_PLUS_THIRD_DOSE_PCT",
                        "title": "Percent of Population",
                        "type": "quantitative"
                    }
                ],
                "color": {
                    "datum": "Third Dose"
                }
            }
        }
    ]
}